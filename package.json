    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "hooks": "^0.3.2",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "prisma": "^3.9.2",
