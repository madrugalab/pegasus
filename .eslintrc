'prettier',
'prettier/react'.
rules: {
    'prettier/prettier': 'error',
    'react/jsx-filename-extension': [
      'warn',
        {extensions: ['.js', '.jsx'
            ]
        }
    ],
    'import/prefer-default-export': 'off',
    'jsx-quotes': ['error', 'prefer-single'
    ]
},
{
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true
}